                span 2.10x
              a.graph-label(href='#' style='background: #22B56B')
                span 2.10x
          .game-timer 09:20
          .game-statics
            .game-statics__player
              .tit Игроки
              .num 237
            .game-statics__bank
              .tit Банк
              .num 2605
            .game-statics__skins
              .tit Скины
              .num 2605
            .game-statics__history
              a.link-history(href="#")
                +svgMono('i-history')
                | История
      .m-col-5
        .block-gr.game-info
          .game-info__title Авто-вывод
          .game-info__koef-row
            .b-enter
              input(type="text" placeholder="Введите КФ")
            .b-edit
              button.edit
                +svgMono('i-setting')
                | Изменить
          ul.game-info__check_koef
            li
              button.active 1.1x
            li
              button 1.5x
            li
              button 2x
            li
              button 3x
            li
              button 5x
          .game-info__cols
            .game-info__rate
              .title Ставка
              .rate-text 1512
            .game-info__wins
              .title Выигрыш
              .wins-text 1705
          .game-info__bottom
            button.btn Сделать ставку

      .block-gr.block-inventar.block-sm
        if aut == true
          .b-inventar
            .inventar-title Выбрано
            .inventar-top
              .balance
                img(src='img/valuta.svg')
                .cost 21.54
              .change
                img(src='img/i-gun.svg')
                .cost 34
              .all-check
                span.text Выбрать всё
                label.checker(for='id1')
                  input#id1(type='checkbox')
                  span
                    img(src='img/galka.svg')
            // end invertar-top
            .inventar-cont
              .inventar-list
                .inventar-scroll
                  +invent('active', 'blue')
                  +invent('', 'red')
                  +invent('', 'pink')
                  +invent('', 'blue')
                  +invent('', 'blue')
                  +invent('', 'blue')

                // end invent-scroll
              // end inventar-list
            // end inventar-cont
            .inventar-bottom
              button.btn.btn-exchange.js-open-market
                +svgMono('i-market')
                | Магазин
              button.btn.btn-green.btn-dw
                +svgMono('i-dw')
            // end inventar-bottom
          // end b-inventar
        else
          .inventar-locked
            .inventar-locked__center
              .text
                a(href='#') Авторизуйтесь
                |  на сайте чтобы использовать
              .b-btn
                button.btn-steam
                  +svgMono('steam')
                  | Войти через Steam
          // end inventar-locked
        // end block

