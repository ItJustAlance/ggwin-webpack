.wrapper-content
  .sidebar-left
    .block-gr.b-slider-inventar
      .slider-inventar
        .price-inv
          img(src='img/valuta.svg')
          | 1704
        .inventar-delete
          +svgMono('delete')

    .block-gr.block-inventar
      if aut == true
        .b-inventar
          .inventar-title Выбрано
          .inventar-top
            .balance
              img(src='img/valuta.svg')
              .cost 21.54
            .change
              img(src='img/i-gun.svg')
              .cost 34
            .all-check
              span.text Выбрать всё
              label.checker(for='id2')
                input#id2(type='checkbox')
                span
                  img(src='img/galka.svg')
          // end invertar-top
          .inventar-cont
            .inventar-list
              .inventar-scroll
                +invent('active', 'blue')
                +invent('', 'red')
                +invent('', 'pink')
                +invent('', 'blue')
                +invent('', 'blue')
                +invent('', 'blue')

              // end invent-scroll
            // end inventar-list
          // end inventar-cont
          .inventar-bottom
            button.btn.btn-exchange.js-open-market
              +svgMono('i-market')
              | Магазин
            button.btn.btn-green.btn-dw
              +svgMono('i-dw')
          // end inventar-bottom
        // end b-inventar
      else
        .inventar-locked
          .inventar-locked__center
            .text
              a(href='#') Авторизуйтесь
              |  на сайте чтобы использовать
            .b-btn
              button.btn-steam
                +svgMono('steam')
                | Войти через Steam
        // end inventar-locked
  // end block


  .content
    .game-block
      .m-col-7
        .block-gr.b-game
          .game-bg
            img(src="img/game.png")
          .b-row-labels
            .graph-labels
              a.graph-label(href='#' style='background: #C50D2C')
                span 2.10x
              a.graph-label(href='#' style='background: #A500A1')
                span 2.10x
              a.graph-label(href='#' style='background: #FF9900')
                span 2.10x
              a.graph-label(href='#' style='background: #A500A1')
                span 2.10x
